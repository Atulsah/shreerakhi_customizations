 "html": "<style>\r\n  .label-sheet {\r\n    width: 210mm;       /* A4 width */\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    border: none !important;\r\n    outline: none !important;\r\n  }\r\n  .label {\r\n    width: 65mm;        /* each sticker width */\r\n    height: 35mm;       /* each sticker height */\r\n    text-align: center;\r\n    padding: 5mm 4mm;\r\n    box-sizing: border-box;\r\n    border: none !important;\r\n    outline: none !important;\r\n  }\r\n  .label img {\r\n    border: none !important;\r\n    outline: none !important;\r\n    display: block;\r\n    margin: auto;\r\n    max-width: 100%;\r\n    max-height: 80%;\r\n  }\r\n\r\n</style>\r\n\r\n<div class=\"label-sheet\">\r\n    {% for row in doc.items %}\r\n        {# --- Barcode lookup: check Item Barcode table for UOM --- #}\r\n        {% set barcode = frappe.db.get_value(\r\n            \"Item Barcode\",\r\n            {\"parent\": row.item_code, \"uom\": row.uom},\r\n            \"barcode\"\r\n        ) %}\r\n\r\n        {% for i in range(row.qty|int) %}\r\n            <div class=\"label\">\r\n                <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ barcode or row.item_code }}&code=Code128&dpi=96&hideborder=true\" \r\n                     alt=\"Barcode\">\r\n                <div>{{ row.item_name }} for UOM: {{ row.uom }} </div>\r\n            </div>\r\n        {% endfor %}\r\n    {% endfor %}\r\n</div>",
